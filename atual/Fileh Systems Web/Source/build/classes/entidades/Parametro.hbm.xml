<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="entidades">
	<class name="Parametro" table="PARAMETRO">
		<id name="Par_cd_parametro" 					column="PAR_CD_PARAMETRO">
			<generator class="native" />
		</id>

		<property name="Par_cd_usuario" 			column="PAR_CD_USUARIO" 			not-null="true" />
		
		<property name="Par_ds_parametro" 			column="PAR_DS_PARAMETRO" 			not-null="true" />
		<property name="Par_vl_parametro" 			column="PAR_VL_PARAMETRO" 			not-null="true" />
		
	</class>
</hibernate-mapping>
