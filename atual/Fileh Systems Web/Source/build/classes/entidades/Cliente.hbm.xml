<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="entidades">
	<class name="Cliente" table="CLIENTE">
		<id name="Cli_cd_cliente" 					column="CLI_CD_CLIENTE">
			<generator class="native" />
		</id>

		<property name="Cli_cd_usuario" 			column="CLI_CD_USUARIO" 			not-null="true" />
		
		<property name="Cli_nm_cliente" 			column="CLI_NM_CLIENTE" 			not-null="true" />
		<property name="Cli_ds_logradouro" 			column="CLI_DS_LOGRADOURO" 			not-null="true" />
		<property name="Cli_ds_bairro" 				column="CLI_DS_BAIRRO" 				not-null="true" />
		<property name="Cli_ds_uf" 					column="CLI_DS_UF" 					not-null="true" />
		<property name="Cli_nr_tel" 				column="CLI_NR_TEL" 				not-null="true" />
		<property name="Cli_ds_entidade" 			column="CLI_DS_ENTIDADE" 			not-null="true" />
		<property name="Cli_ds_referencia" 			column="CLI_DS_REFERENCIA" 			not-null="true" />
		<property name="Cli_nr_cpf" 				column="CLI_NR_CPF" 				not-null="true" />
		
	</class>
</hibernate-mapping>
