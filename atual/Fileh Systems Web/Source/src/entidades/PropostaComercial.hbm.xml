<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="entidades">
	<class name="PropostaComercial" table="PROPOSTA_COMERCIAL">
		<id name="Prc_cd_proposta_comercial" 					column="PRC_CD_PROPOSTA_COMERCIAL">
			<generator class="native" />
		</id>

		<property name="Prc_cd_usuario" 			column="PRC_CD_USUARIO" 			not-null="true" />
		
		<property name="Prc_cd_cliente" 			column="PRC_CD_CLIENTE" 			/>
		<property name="Prc_vl_desconto" 			column="PRC_VL_DESCONTO" 			not-null="true" />
		<property name="Prc_vl_frete" 				column="PRC_VL_FRETE" 				not-null="true" />
		<property name="Prc_dt_cadastro" 			column="PRC_DT_CADASTRO" 			/>
		<property name="Prc_dt_prevista" 			column="PRC_DT_PREVISTA" 			/>
		<property name="Prc_ds_obs" 				column="PRC_DS_OBS" 				/>
		<property name="Prc_nm_atendente" 			column="PRC_NM_ATENDENTE"			/>
		<property name="Prc_ds_situacao" 			column="PRC_DS_SITUACAO"			/>
		<property name="Prc_ds_forma_pagamento" 	column="PRC_DS_FORMA_PAGAMENTO"		/>
		
	</class>
</hibernate-mapping>
