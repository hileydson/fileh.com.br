<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="entidades">
	<class name="ItemProposta" table="ITEM_PROPOSTA">
		<id name="Ipc_cd_item_proposta" 					column="IPC_CD_ITEM_PROPOSTA">
			<generator class="native" />
		</id>

		<property name="Ipc_cd_proposta_comercial" 			column="IPC_CD_PROPOSTA_COMERCIAL" 		not-null="true" />
		
		<property name="Ipc_ds_item_proposta" 				column="IPC_DS_ITEM_PROPOSTA" 			not-null="true" />
		<property name="Ipc_vl_item_proposta" 				column="IPC_VL_ITEM_PROPOSTA" 			not-null="true" />
		<property name="Ipc_nr_quantidade" 					column="IPC_NR_QUANTIDADE" 				not-null="true" />
		<property name="Ipc_vl_desconto" 					column="IPC_VL_DESCONTO" 				not-null="true" />
		<property name="Ipc_ds_unidade" 					column="IPC_DS_UNIDADE" 				not-null="true" />
		
	</class>
</hibernate-mapping>
