<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="entidades">
	<class name="Entidade" table="ENTIDADE">
		<id name="Ent_cd_entidade" 					column="ENT_CD_ENTIDADE">
			<generator class="native" />
		</id>

		<property name="Ent_cd_usuario" 			column="ENT_CD_USUARIO" 			not-null="true" />
		
		<property name="Ent_nm_entidade" 			column="ENT_NM_ENTIDADE" 			not-null="true" />
		
	</class>
</hibernate-mapping>
